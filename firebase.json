{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/*.md",
      "index.html"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/login.html"
      },
      {
        "source": "**",
        "destination": "/login.html"
      }
    ],
    "redirects": [
      {
        "source": "/index.html",
        "destination": "/login.html",
        "type": 301
      }
    ]
  }
}
